{
  "name": "easii-frontend",
  "version": "1.0.0",
  "description": "This is a tool to triaging issues.",
  "main": "index.js",
  "scripts": {
    "test": "jest ./src",
    "clean": "rimraf dist node_modules",
    "build": "webpack --colors --config ./webpack.config.prod.js",
    "start": "npm run build && node --optimize_for_size --max_old_space_size=460 --gc_interval=100 server.js",
    "start-dev": "webpack-dev-server --progress --colors --config ./webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaurav-quasar/Boilerplate.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Krishna Kamal Nath",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gaurav-quasar/Boilerplate/issues"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "homepage": "https://github.com/gaurav-quasar/Boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.1",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "bootstrap": "^4.5.3",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-social-icons": "^4.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "./src/enzyme.setup.js",
    "setupFiles": [
      "raf/polyfill"
    ]
  }
}
